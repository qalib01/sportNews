<%- include('./_partials/header') %>
  <!-- Content wrapper -->
  <div class="content-wrapper">
    <!-- Content -->
    <div class="container-xxl flex-grow-1 container-p-y">
      <div class="row">
        <div class="col-lg-6 mb-4 order-0">
          <div class="card">
            <div class="d-flex align-items-end row">
              <div class="col-12">
                <div class="card-body">
                  <h5 class="card-title text-primary">Xoş gəldin!</h5>
                  <p class="mb-4">
                    Yolumuz hələ uzundur. Çalışmaq lazımdır, istifadəçilərimizin yeni xəbərlərə ehtiyacları var!
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-lg-6 mb-4 order-0">
          <div class="card">
            <div class="d-flex align-items-end row">
              <div class="col-12">
                <div class="card-body px-0">
                  <div class="tab-content p-0">
                    <div class="tab-pane fade show active" id="navs-tabs-line-card-income" role="tabpanel">
                      <div class="d-flex p-4 pt-3">
                        <div>
                          <small class="text-muted d-block"><%= analytics[0].dimensions.date %> tarixli giriş sayı</small>
                          <div class="d-flex align-items-center">
                            <h6 class="mb-0 me-1"><%= analytics[0].uniq.uniques %></h6>
                          </div>
                        </div>
                      </div>
                      <div id="incomeChart"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- / Content -->

    <%- include('./_partials/footer') %>